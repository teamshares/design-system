{
  "name": "@teamshares/ui",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist/*.js",
    "tailwind.config.js",
    "images/*",
    "scss/*.scss"
  ],
  "scripts": {
    "lint_js": "eslint --ext .js,.jsx,.ts,.tsx src/",
